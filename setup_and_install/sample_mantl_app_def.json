{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "DOCKERUSER/DOCKERREPO:latest",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 5000,
                "hostPort": 0
            }]
        },
        "forcePullImage": true
    },
    "healthChecks": [
        {
        "protocol": "TCP",
        "portIndex": 0
        },
        {
        "path": "/health",
        "protocol": "HTTP"
      }
    ],
    "id": "/DEPLOYMENTDIR/BOTNAME/USERNAME",
    "instances": 1,
    "cpus": 0.1,
    "mem": 16,
    "env": {
        "SPARK_BOT_URL": "http://DEPLOYMENTDIR-BOTNAME-USERNAME.APPDOMAIN",
        "SPARK_BOT_APP_NAME": "BOTNAME",
        "SPARK_BOT_TOKEN": "BOTTOKEN",
        "SPARK_BOT_EMAIL": "BOTEMAIL",
        "CASE_API_CLIENT_ID": "CLIENTID",
        "CASE_API_CLIENT_SECRET": "CLIENTSECRET",
        "FEEDBACK_ROOM": "FEEDBACKROOM"
    }
}
